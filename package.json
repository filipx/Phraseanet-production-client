    "bootstrap-multiselect": "^0.9.13-1",
    "bootstrap-sass": "^2.3.2",
    "es6-promise": "^3.0.2",
    "geonames-server-jquery-plugin": "^0.2.2",
    "humane-js": "^3.2.2",
    "i18next": "^2.1.0",
    "i18next-xhr-backend": "^0.5.3",
    "jquery": "^1.11.3",
    "jquery-lazyload": "^1.9.7",
    "jquery-treeview": "https://github.com/alchemy-fr/jquery-treeview.git",
    "jquery-ui": "^1.10.5",
