  },
  "author": {
    "name": "Florian Blouet",
    "email": "florian.blouet@gmail.com"
  },
  "bugs": "https://github.com/alchemy-fr/Phraseanet-production-client/issues",
  "homepage": "https://github.com/alchemy-fr/Phraseanet-production-client",
  "license": "MIT",
